{
    "name": "tuding-boilerplate",
    "version": "1.1.2",
    "description": "TudingBus Client Portal",
    "browserslist": [
        "defaults",
        "last 10 versions"
    ],
    "scripts": {
        "doc": "esdoc",
        "add-locale": "lingui add-locale en_US zh_CN it_IT fr_FR de_DE es_ES",
        "extract": "lingui extract --clean",
        "compile": "lingui compile",
        "eslint": "./node_modules/.bin/eslint ./src/**/*.js",
        "build": "npm run compile && cross-env NODE_ENV=production webpack -p --profile --optimize-minimize",
        "build-major": "npm run compile && cross-env NODE_ENV=production webpack -p --profile --optimize-minimize --env.major",
        "build-minor": "npm run compile && cross-env NODE_ENV=production webpack -p --profile --optimize-minimize --env.minor",
        "build-patch": "npm run compile && cross-env NODE_ENV=production webpack -p --profile --optimize-minimize --env.patch",
        "start": "cross-env NODE_ENV=production babel-node ./server.js",
        "dev": "npm run compile && concurrently --kill-others \"nodemon ./server.js --exec babel-node\" \"webpack-dev-server\""
    },
    "lingui": {
        "localeDir": "<rootDir>/locale",
        "srcPathDirs": [
            "<rootDir>/src"
        ]
    },
    "keywords": [],
    "author": "shye0000",
    "license": "ISC",
